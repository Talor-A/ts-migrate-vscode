{
  "name": "tsmigratevscode",
  "displayName": "ts-migrate for VS Code",
  "description": "wrapper around airbnb/ts-migrate for VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.53.0"
  },
  "publisher": "talor-a",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tsmigratevscode.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tsmigratevscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "version": "npm version `auto version` -m 'Bump version to: %s [skip ci]'",
    "publish": "npm publish && git push --follow-tags --set-upstream origin $branch",
    "release": "auto changelog && npm run version && npm run publish && auto release"
  },
  "devDependencies": {
    "@auto-it/all-contributors": "^10.16.7",
    "@auto-it/first-time-contributor": "^10.16.7",
    "@auto-it/vscode": "^10.16.7",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.53.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "auto": "^10.16.7",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.2.2",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "path": "^0.12.7",
    "ts-migrate-plugins": "^0.1.15",
    "ts-migrate-server": "^0.1.15"
  },
  "repository": "Talor-A/ts-migrate-vscode",
  "author": "talor (via auto) <ta.anders17@gmail.com>",
  "auto": {
    "plugins": [
      "vscode",
      "all-contributors",
      "first-time-contributor",
      "released"
    ]
  }
}
